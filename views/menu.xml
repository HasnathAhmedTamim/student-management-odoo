<odoo>
    <record id="action_student_management_student" model="ir.actions.act_window">
        <field name="name">Students</field>
        <field name="res_model">student.management.student</field>
        <field name="view_mode">list,form</field>
    </record>
    <record id="action_student_management_course" model="ir.actions.act_window">
        <field name="name">Courses</field>
        <field name="res_model">student.management.course</field>
        <field name="view_mode">list,form</field>
    </record>
    <menuitem id="student_management_menu" name="Student Management" sequence="10"/>
    <menuitem id="student_management_menu_students" name="Students" parent="student_management_menu" action="action_student_management_student"/>
    <menuitem id="student_management_menu_courses" name="Courses" parent="student_management_menu" action="action_student_management_course"/>
</odoo>